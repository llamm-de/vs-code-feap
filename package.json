{
    "name": "vs-code-feap-syntax",
    "displayName": "vs_code_feap_syntax",
    "description": "Syntax highlighting for feap related files",
    "version": "0.0.1",
    "publisher": "llamm-de",
    "repository": {
        "type": "git",
        "url": "https://github.com/llamm-de/vs-code-feap.git"
    },
    "engines": {
        "vscode": "^1.55.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [
            {
                "id": "feap",
                "aliases": [
                    "FEAP",
                    "feap"
                ],
                "extensions": [],
                "firstLine": "^FEAP.*",
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "feap",
                "scopeName": "source.feap",
                "path": "./syntaxes/feap.tmLanguage.json"
            }
        ]
    },
    "dependencies": {}
}
